{
    "name": "书签去重复",
    "version": "1.0",
    "description": "自动删除重复的书签",
    "permissions": [
        "bookmarks"
    ],
    "browser_action": {
        "default_title": "书签去重复",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}